[application]

config/name="Deep Space Beat"
run/main_scene="res://screens/BootScreen/BootScreen.tscn"
boot_splash/image="res://assets/images/splash.png"
boot_splash/fullsize=false
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"
config/windows_native_icon="res://assets/images/icon.ico"

[autoload]

GameLoadCache="*res://autoloads/GameLoadCache.gd"
GameState="*res://autoloads/GameState.gd"
GameDebugPanel="*res://autoloads/GameDebugPanel.tscn"
GameSceneTransitioner="*res://autoloads/GameSceneTransitioner.tscn"

[debug]

gdscript/warnings/return_value_discarded=false

[display]

window/size/width=1280
window/size/height=720

[engine]

version="3.4.4"

[global]

theme=false
